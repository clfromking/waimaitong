/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.left{
  float: left
}
.right{
  float: right
}
.clear:after{
  display:block;
  clear:both;
  content:"";
  visibility:hidden;
  height:0
}
.clear{
  zoom:1
}
