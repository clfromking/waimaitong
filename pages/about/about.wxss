.logo{
  width: 186rpx;
  height: 186rpx;
  border-radius: 50%;
  /* background: #7d7d7d; */
  display: block;
  margin: 40rpx auto 0
}
.version{
  font-size: 30rpx;
  color: #999;
  text-align: center;
  margin-top: 42rpx;
  
}
.goIntroduce{
  background: #eeeeee;
  padding: 0 40rpx;
  font-size: 30rpx;
  color: #333;
  height: 106rpx;
  margin-top: 48rpx
}
.go{
  width: 22rpx;
  transform: rotate(180deg);
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  margin: auto
}
.goIntroduce .left{
  height: 106rpx;
  line-height: 106rpx;

}
.goIntroduce .right{
  height: 106rpx;
  position: relative
}