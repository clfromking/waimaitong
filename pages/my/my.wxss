/* pages/my/my.wxss */
page{
  background: #eee;
  padding-bottom: 50rpx
}
.jw-my-header{
  height: 542rpx;
  background: #abca9a;
  position: relative
}
.jw-user-msgs{
  width: 248rpx;
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 30rpx;
  margin: auto 
  /* height: 56rpx */
}
.jw-user-avatarurl{
  width: 126rpx;
  height: 126rpx;
  display: block;
  border-radius: 50%;
  background: #fff;
  margin: 0 auto 24rpx
}
.login-alt{
  font-size: 36rpx;
  color: #fff;
  font-weight: bold;
  display: block;
  text-align: center
}
.dredge-member{
  width: 100%;
  height: 56rpx;
  background-color: #ffffff;
	border-radius: 28rpx;
  text-align: center;
  color: #880000;
  font-size: 30rpx;
  line-height: 56rpx;
  font-weight: bold;
  margin-top: 26rpx
}
.user-moneys{
  height:166rpx;
  /* background:red; */
  position: absolute;
  bottom: 0;
  width: 100%
}
.moneys-item{
  display: inline-block;
  text-align: center;
  width: 373rpx;
  /* margin-top: 10rpx */
  /* height: 100% */
}
.moneys-num{
  font-size: 48rpx;
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  
}
.moneys-alt{
  color: #fff;
  font-size: 30rpx
}
.fenge{
  width: 4rpx;
  background: #fff;
  height: 98rpx;
  display: inline-block
}
.body-list-options{
  height: 108rpx;
  background: #fff;
  margin-bottom: 18rpx
}
/* .body-list-options:last-child{
  margin-bottom: 50rpx
} */
.options-icon{
  width: 84rpx;
  height: 84rpx;
  margin: 12rpx 26rpx 0 28rpx;
  display: inline-block
}
.options-icon-img{
  width: 100%;
  height: 100%;
  display: block
}
/* .options-icon{
  width: 44rpx;
  height: 44rpx;
  margin: 40rpx 46rpx 0 48rpx
} */
.options-text,.options-other{
  display: inline-block;
  height: 100%;
  color: #333333;
  font-size: 30rpx;
  line-height: 108rpx;
  vertical-align: top;

}
.options-go{
  width: 16rpx;
  height: 28rpx;
  /* background: red; */
  margin: 40rpx 32rpx 0 44rpx;
  display: inline-block;
  transform: rotate(180deg)
}

.islogin{
  position: absolute;
  width: 100vw;
  height: 100%;
  /* background: #000; */
  z-index: 9;
  top: 0;
  left: 0
}
.coverBtn{
  width: 100%;
  position: absolute;
  height: 108rpx;
  /* background: red; */
  top: 0;
  left: 0;
  opacity: 0
}
.coverBtn::after{
  border: none
}

.z_index{
  z-index: 10
}