/* pages/order/order.wxss */
page{
  background: #eee
}
.jw-order-nologin{
  text-align: center;
  width: 200rpx;
  position: absolute;
  top: 96rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto
}
.jw-order-nav{
  background: #3A4BF9;
  font-size: 36rpx;
  color: #fff;
  padding-left: 20rpx;
  height: 56rpx;
  line-height: 56rpx;
  display: none
  /* margin-left: 20rpx */
}
.jw-order-nav text{
   height: 56rpx;
  line-height: 56rpx;
  display: inline-block
}
.jw-nologin-img{
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin-bottom: 40rpx
}
.jw-nologin-alt{
  font-size: 30rpx;
  color: #333333;
  /* margin-top: 40rpx 0 102rpx; */
  display: inline-block
}
.jw-nologin-btn{
  width: 202rpx;
  height: 62rpx;
  background-color: #3A4BF9;
	border-radius: 32rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  line-height: 62rpx;
  margin-top: 102rpx
}
.jw-orders-view{
  width: 728rpx;
  margin: 0 auto
}
.jw-order-item{
  background: #fff;
  border-radius: 10rpx;
  position: relative;
  padding: 0 26rpx;
  box-sizing: border-box;
  margin-top: 24rpx
}
.jw-order-item-top{
  height: 52rpx;
  /* background: aqua */
}
.jw-order-time,.jw-order-status{
  font-size: 24rpx;
  color: #999;
  display: inline-block;
  height: 52rpx;
  line-height: 52rpx;
  /* margin-left: 26rpx; */
  vertical-align: top;
}

.jw-delete-btn{
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  background: url('https://waimaitong.oss-cn-beijing.aliyuncs.com/wechat/all/delete.png') no-repeat;
  background-size: 100% 100%;
  margin-top: 8rpx;
  /* margin-right: 26rpx */
}
.shu{
  color: #999;
  font-size: 24rpx;
  line-height: 52rpx;
  vertical-align: top;
  margin:0 12rpx
}

.jw-order-item-lists{
  margin: 24rpx 0
}
/* .jw-order-item-lists:first{
  margin-top: 34rpx 0
}
.jw-order-item-lists:first-child{
  
}
.jw-order-item-lists:last-child{
  margin-bottom: 34rpx 0
} */
.jw-lists-img-view{
  width: 150rpx;
  height: 102rpx;
  border-radius: 10rpx;
  /* display: block; */
}
.jw-lists-img{
  width: 100%;
  height: 100%;
  display: block
}
.jw-lists-msgs-view{
  width: 526rpx;
  /* background: red */
}
.jw-lists-name{
  margin-left: 22rpx
}
.jw-lists-name,.jw-lists-number{
  display: inline-block;
  height: 102rpx;
  line-height: 102rpx;
  font-size: 24rpx;
  color: #333;
  vertical-align: top;
  max-width: 350rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.jw-lists-number{
  margin-right: 26rpx;
  display: inline-block;

}
.jw-order-price{
  color: #333;
  font-size: 24rpx;
  text-align: right;
  padding-right: 26rpx;
  margin-bottom: 22rpx
}
.fenge{
  height: 2rpx;
  background: #dcdcdc
}
.jw-order-item-bottom{
  height: 80rpx;
  line-height: 80rpx;
}
.jw-order-number{
  color: #191919;
  font-size: 24rpx;
  height: 100%
}
.jw-order-method_views{
  display: inline-block
}
.jw-order-detail{
  width: 148rpx;
	height: 46rpx;
	border-radius: 8rpx;
	border: solid 4rpx #3A4BF9;
  font-size: 24rpx;
  text-align: center;
  line-height: 46rpx;
  /* margin-top: -24rpx; */
  position: absolute;
  top: 14rpx;
  right: 0;
  color: #3A4BF9
}
.jw-order-payMethod{
  position: absolute;
  top: 0rpx;
  right: 180rpx;
  font-size: 24rpx;
  color: #191919;
  width: 148rpx;
  text-align: center
}
.nothing{
  width: 491rpx;
  height: 514rpx;
  display: block;
  margin: 98rpx auto 110rpx
}
.nothing-title{
  color: #3A4BF9;
  font-size: 40rpx;
  text-align: center;
}
.nothing-btn{
  width: 160rpx;
  height: 48rpx;
  border-radius: 8rpx;
  background: #3ABBF9;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 48rpx;
  margin: 44rpx auto 0
}
.load{
  width: 100vw;
  height: 100vh;
  position: fixed;
  /* top: 0; */
  left: 0;
  background: #fff
}