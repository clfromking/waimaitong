/* pages/storeDecorate/storeDecorate.wxss */
.notice{
  height: 64rpx;

}
.notice image,.notice text{
  display: block;
}
.notice image{
  width: 48rpx;
  height: 48rpx;
  margin-top: 8rpx;
  margin-left: 42rpx
}
.notice text{
  height: 64rpx;
  line-height: 64rpx;
  font-size: 24rpx;
  color: #2d2d2d
}
.container{
  width: 670rpx;
  margin: 0 auto
}
.item{
  height: 192rpx;
  border-bottom: 8rpx solid #f6f6f6
}
.item .left{
  margin-top: 20rpx
}
.item-imgView{
  width: 188rpx;
  height: 152rpx;
  margin-right: 38rpx;
  
}
.item-imgView image{
  width: 100%;
  height: 100%;
  display: block
}
.name{
  color: #2d2d2d;
  font-size: 36rpx;
  font-weight: bold;
  
}
.price{
  font-size: 24rpx;
  margin-top: 28rpx;
  color: #2d2d2d;
}
.mPrice{
  font-size: 28rpx;
  color: #ff6959
}
.bottom{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #5a5a5a;
  text-align: center;
  padding-bottom: 165rpx
}
.footer{
  position: fixed;
  height: 104rpx;
  width: 750rpx;
  bottom: 0;
  left: 0;
  background: #abca9a
}
.add-btn{
  width: 156rpx;
  height: 156rpx;
  border-radius: 50%;
  background-image: radial-gradient(50% 102%, #BEE9A7 50%, #ABCA9A 100%);
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  font-weight: bold;
  position: fixed;
  bottom: 26rpx;
  left: 40rpx
}
.add-btn view{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 72rpx;
  height: 100rpx
}
.PriceView{
  position: absolute;
  top: 4rpx;
  left: 252rpx;
  color: #2d2d2d
}
.acPrice{
  font-size: 36rpx;
  font-weight: bold
}
.disPrice{
  font-size: 24rpx;
  margin-top: 4rpx
}
.submit-order{
  height: 104rpx;
  line-height: 104rpx;
  color: #fff;
  font-size: 36rpx;
  font-weight: bold;
  position: absolute;
  right: 50rpx
}